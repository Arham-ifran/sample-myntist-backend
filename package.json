{
  "name": "myntist-backend",
  "version": "1.0.0",
  "description": "One of the largest NFT marketplace for buying and selling NFTs. Some of the blockchain networks compatible with this marketplace are Binance and MYNT token. This is a NFT Marketplace for users to choose the NFT they want to invest in, sell and buy NFTs.",
  "author": "Arham",
  "main": "src/index.js",
  "private": false,
  "license": "MIT",
  "engines": {
    "node": ">=8",
    "yarn": "*"
  },
  "scripts": {
    "start": "cross-env NODE_ENV=production pm2 start ./src/index.js -i max",
    "dev": "nodemon ./src/index.js",
    "pm2": "pm2 start ./src/index.js --name=myntist-backend",
    "pm2-restart": "pm2 restart myntist-backend",
    "pm2-stop": "pm2 stop myntist-backend",
    "pm2-list": "pm2 list",
    "pm2-delete": "pm2 delete myntist-backend"
  },
  "keywords": [
    "IPFS",
    "MYNT",
    "NFT",
    "Marketplace",
    "Nodejs",
    "API"
  ],
  "dependencies": {
    "atob": "^2.1.2",
    "axios": "^0.24.0",
    "bcryptjs": "^2.4.3",
    "compression": "^1.7.4",
    "cors": "^2.8.5",
    "crypto-js": "^4.1.1",
    "csv-parser": "^3.0.0",
    "dotenv": "^10.0.0",
    "download": "^8.0.0",
    "ethers": "^5.5.4",
    "express": "^4.17.1",
    "express-bearer-token": "^2.4.0",
    "express-rate-limit": "^5.3.0",
    "express-validation": "^3.0.8",
    "express-validator": "^6.14.0",
    "hex2dec": "^1.1.2",
    "html5-to-pdf": "^4.0.1",
    "http-status": "^1.5.0",
    "ipfs-http-client": "^51.0.1",
    "jszip": "^3.10.1",
    "jwt-simple": "^0.5.6",
    "mailgun-js": "^0.22.0",
    "method-override": "^3.0.0",
    "mime-type": "^4.0.0",
    "moment": "^2.29.1",
    "moment-timezone": "^0.5.34",
    "mongoose": "^5.13.7",
    "multer": "^1.4.3",
    "passport": "^0.4.1",
    "passport-local": "^1.0.0",
    "paypal-rest-sdk": "^1.8.1",
    "pdfkit": "^0.13.0",
    "pm2": "^5.1.1",
    "randomatic": "^3.1.1",
    "socket.io": "^4.4.1",
    "stripe": "^11.15.0",
    "tinify": "^1.6.1",
    "twilio": "^3.67.1",
    "web3": "^1.7.0",
    "web3-eth-contract": "^1.6.1",
    "zip-dir": "^2.0.0"
  }
}
